{
  "name": "repeat-runner",
  "version": "0.3.5",
  "description": "A javascript tool for run repeat code.",
  "main": "dist/repeat-runner.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "babel src/ --out-dir dist/",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "nyc": {
    "watermarks": {
      "lines": [80, 95],
      "functions": [80, 95],
      "branches": [80, 95],
      "statements": [80, 95]
    },
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "report-dir": "./test/coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huanguolin/repeat-runner.git"
  },
  "keywords": [
    "repeat",
    "runner",
    "js"
  ],
  "author": "Alvin Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Huanguolin/repeat-runner/issues"
  },
  "homepage": "https://github.com/Huanguolin/repeat-runner#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rename-umd-globals": "^1.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "codecov": "^2.1.0",
    "cross-env": "^4.0.0",
    "mocha": "^3.3.0",
    "mochawesome": "^2.0.5",
    "nyc": "^10.2.0"
  }
}
